{
    "name": "florianwolters/component-core-equality",
    "description": "Implements equivalence relations on non-null object references as a PHP component.",
    "type": "library",
    "keywords": ["equality", "equals", "reference equality", "value equality", "comparison", "core"],
    "homepage": "http://github.com/FlorianWolters/PHP-Component-Core-Equality",
    "license": "LGPL-3.0+",
    "authors": [
      {
        "name": "Florian Wolters",
        "email": "wolters.fl@gmail.com",
        "homepage": "http://blog.florianwolters.de",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpmd/phpmd": "2.*",
        "phpunit/phpunit": "4.*",
        "sebastian/phpcpd": "2.*",
        "sebastian/phpdcd": "1.*",
        "sami/sami": "1.*",
        "satooshi/php-coveralls": "0.*",
        "sensiolabs/security-checker": "1.*",
        "squizlabs/php_codesniffer": "2.*@dev"
    },
    "autoload": {
        "psr-4": {
            "FlorianWolters\\Component\\Core\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlorianWolters\\Component\\Core\\": "src/test/php",
            "FlorianWolters\\Example\\": "src/test/resources"
        }
    }
}
